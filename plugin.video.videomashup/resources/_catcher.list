########################################################
# VideoMashup Catcher list
########################################################
# title: str <- Start flag
# target: RegEx <- Start flag
# subs: RegEx
# ext_subs: RegEx
# subsurl: *%s*
# ext_subsurl: *%s*
# url: *%s*
# data: *%s*
# header str|str
# limit: int
# action: str
# build: *%s*
# ext_target: RegEx
# ext_url: *%s*
# ext_data: *%s*
# ext_header str|str
# ext_limit: int
# ext_action: str
# ext_build: *%s*
# extension: str (default = flv)
# info: str
# quality: low, standard, high or fallback <- End flag
########################################################
# Catcher sites
########################################################
title=tuga.io
target=file: '(.+?)',
subs=file: "(.+?)",\s+default: "true"
subsurl=http://tuga.io%s
url=%s
quality=high
########################################################
