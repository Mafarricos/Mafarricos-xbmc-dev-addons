########################################################
# VideoDevil Catcher list
########################################################
# title: str <- Start flag
# target: RegEx <- Start flag
# subs: RegEx
# ext_subs: RegEx
# subsurl: *%s*
# ext_subsurl: *%s*
# url: *%s*
# data: *%s*
# header str|str
# limit: int
# action: str
# build: *%s*
# ext_target: RegEx
# ext_url: *%s*
# ext_data: *%s*
# ext_header str|str
# ext_limit: int
# ext_action: str
# ext_build: *%s*
# extension: str (default = flv)
# info: str
# quality: low, standard, high or fallback <- End flag
########################################################
# Catcher sites
########################################################
title=tuga.io
target=file: '(.+?)',
subs=file: "(.+?)",\s+default: "true"
subsurl=http://tuga.io%s
url=%s
quality=high
########################################################
title=kids.tuga.io
target=file: '(.+?)',
subs=file: "(.+?)",\s+default: "true"
subsurl=http://tuga.io%s
url=%s
quality=high
########################################################
title=gigatuga.com
target=data-mfp-src="https://openload.io(.+?)"
info=openload
url=%s
build=https://openload.io%s
ext_target="https:\\/\\/openload.io\\/stream\\/(.+?)"
ext_url=%s
ext_build=https://openload.io/stream/%s
ext_subs=<track kind="captions" src="(.+?)" srclang=".+?" label=".+?" default></track>
ext_subsurl=http://openload.io%s
quality=high
########################################################
title=tugaflix.com
target=onclick="location.href='(.+?)'"><i class="fa fa-play-circle-o"></i>
url=%s
ext_target="https:\\/\\/openload.io\\/stream\\/(.+?)"
ext_url=%s
ext_build=https://openload.io/stream/%s
ext_subs=<track kind="captions" src="(.+?)" srclang=".+?" label=".+?" default></track>
ext_subsurl=http://openload.io%s
quality=high
########################################################
title=toppt.net
target=data-mfp-src="http://videowood.tv/embed/(.+?)" data-mfp-type="iframe">
url=%s
build=http://videowood.tv/embed/%s
ext_target=file: '(.+?)',
ext_url=%s
quality=high
########################################################

